  "description": "Anthony Cakes Web Application",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run clean && webpack --env=dev --color --progress",
    "build:prod": "npm run clean && webpack --env=prod --color --progress",
    "clean": "rimraf public/dist/*",
    "lint": "tslint --type-check --project ./src/tsconfig.json -c ./src/tslint.json src/**/*.ts --force",
    "start": "node server.js",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch"
  },
